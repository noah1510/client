; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="OpenChamp"
run/main_scene="res://scripts/init/init.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/image="res://default_assets/openchamp/textures/openchamp.png"
config/icon="res://default_assets/openchamp/textures/openchamp.png"

[autoload]

RegistryManager="*res://scripts/registries/registry_manager.gd"
Config="*res://scripts/config.gd"
DebugMenu="*res://addons/debug_menu/debug_menu.tscn"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor]

run/main_run_args="-I -gm openchamp:onslaught"
export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/debug_menu/plugin.cfg", "res://addons/index_manager/plugin.cfg")

[global_group]

Character="A player character"
Map="Valid Map"
Ground="Map Floor"

[gui]

timers/tooltip_delay_sec=0.25

[importer_defaults]

texture={
"compress/mode": 3,
"mipmaps/generate": true
}
scene={
"gltf/embedded_image_handling": 0
}

